{
  "extends": "airbnb",
  "rules": {
    "no-tabs": "off",
    "indent": ["error", "tab", { "SwitchCase": 1 }],
    "no-plusplus": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js"] }],
    "react/jsx-indent": [1, "tab"],
    "react/jsx-indent-props": [1, "tab"]
  },
  "env": {
    "browser": true
  }
}
